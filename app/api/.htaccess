RewriteEngine on

ErrorDocument 404 /pagina-no-encontrada

RewriteRule ^clients$ index.php?table=clients
RewriteRule ^client/(\w*)$ index.php?client=$1

RewriteRule ^inicio$ index.html
RewriteRule ^crud/personas$ crud/personas.php
RewriteRule ^crud/persona/(\w*)$ crud/persona.php?dni=$1
RewriteRule ^crud/anadir$ crud/anadir.php
RewriteRule ^crud/delete/(\w*)$ crud/delete.php?dni=$1
RewriteRule ^crud/update crud/update.php
RewriteRule ^pagina-no-encontrada$ 404.html